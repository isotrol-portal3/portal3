<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="component" class="com.isotrol.impe3.palette.atom.feed.img.ImgComponent"
		scope="prototype">
		<property name="fileLoader" ref="fileLoader" />
		<property name="moduleConfig" ref="moduleConfig" />
		<property name="imgUri" ref="imgUri" />
	</bean>

	<bean id="imgUri" class="com.isotrol.impe3.connectors.uri.EmptyURIBuilderImpl">
	</bean>

</beans>